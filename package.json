{
  "name": "johnson-dot-com",
  "homepage": "https://johnsonryu.github.io/",
  "version": "1.0.0",
  "scripts": {
    "local": "API_TARGET=local nodemon",
    "dev": "API_TARGET=development nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "predeploy": "yarn run build",
    "deploy": "next build && next export && git add out/ && git commit -m \"Deploy gh-pages\" && git subtree push --prefix out origin gh-pages",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/styled-components": "^4.1.16",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "emoji-dictionary": "^1.0.10",
    "express": "^4.16.4",
    "file-loader": "^1.1.11",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.2",
    "next": "^8.1.0",
    "next-cookies": "^2.0.1",
    "next-routes": "^1.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.88.0",
    "styled-components": "^4.3.2",
    "url-loader": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@kadira/react-storybook-addon-info": "^3.4.0",
    "@storybook/react": "^4.1.6",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.22",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-jsx": "^2.2.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^7.0.0",
    "cross-env": "^5.2.0",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.8",
    "prettier": "1.16.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.0",
    "gh-pages": "^2.1.1"
  },
  "license": "ISC"
}
